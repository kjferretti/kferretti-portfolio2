<!-- Modal1 -->
<div class="modal fade" id="properties" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h1 class="modal-title" id="myModalLabel">Properties of a Series</h1>
            </div>
            <div class="modal-body col-sm-12">
                @*<h3>Properties of a series</h3>*@
                <h5>Please enter a number in each box</h5>
                <input type="number" id="inputOneA" />
                <input type="number" id="inputOneB" />
                <input type="number" id="inputOneC" />
                <input type="number" id="inputOneD" />
                <input type="number" id="inputOneE" />
                <button onclick="problemOne()">Submit</button>
                <p id="resultOne">Result will show up here</p>
                @*<div class="row">
                    <div class="col-md-6">*@
                        <pre class="brush: js">
      function seriesFinder(numberOne, numberTwo, numberThree, numberFour, numberFive) {
            
            var seriesArray = [numberOne, numberTwo, numberThree, numberFour, numberFive];
            var currentGreatest = seriesArray[0];
            for (var i = 0; i < seriesArray.length; i++) {
                if (seriesArray[i] > currentGreatest) {
                    currentGreatest = seriesArray[i];
                }
            }
            var greatest = currentGreatest;
            var currentLeast = seriesArray[0];
            for (var i = 0; i < seriesArray.length; i++) {
                if (seriesArray[i] < currentLeast) {
                    currentLeast = seriesArray[i];
                }
            }
            var least = currentLeast;
            var sum = 0;
            var product = 1;
            for (var i = 0; i < seriesArray.length; i++) {
                sum += seriesArray[i];
                product *= seriesArray[i];
            }
            var mean = sum / seriesArray.length;
            
            return 'Least Number: ' + least + ' Greatest Number: ' + greatest + ' Mean: ' + mean + ' Sum: ' + sum + ' Product: ' + product;
 
        }
                                        </pre>
            </div>
            <div class="modal-footer no-margin">
            </div>
        </div>
    </div>
</div>
<!-- /Modal1 -->

<!-- Modal2 -->
<div class="modal fade" id="factorial" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h1 class="modal-title" id="myModalLabel">Factorial</h1>
            </div>
            <div class="modal-body col-sm-12">
                @*<h3>Factorial</h3>*@
                <h5>Please enter a number 0-170 in the box to find its factorial</h5>
                <input type="number" id="inputTwo" />
                <button onclick="problemTwo()">Submit</button>
                <p id="resultTwo">Result will show up here</p>
                @*<div class="row">
                    <div class="col-md-6">*@
                <pre class="brush: js">
       function factorialFinder(input) {
            if (input < 0 || input > 170) {
                return 'Invalid input';
            }
            else if (input > 0) {
                var factorial = 1;
                while (input != 0) {
                    factorial *= input;
                    input = input - 1;
                }
                return 'Factorial: ' + factorial;
            }
            else {
                return 'Factorial: ' + 1;
            }
        }
                                        </pre>
            </div>
            <div class="modal-footer no-margin">
            </div>
        </div>
    </div>
</div>
<!-- /Modal2 -->

<!-- Modal3 -->
<div class="modal fade" id="fizzbuzz" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h1 class="modal-title" id="myModalLabel">FizzBuzz</h1>
            </div>
            <div class="modal-body col-sm-12">
                @*<h3>FizzBuzz</h3>*@
                <h5>Please enter a number 1-100 in each box to find multiples of each</h5>
                <input type="number" id="inputThreeA" />
                <input type="number" id="inputThreeB" />
                <button onclick="problemThree()">Submit</button>
                <p id="resultThree">Result will show up here</p>
                @*<div class="row">
                    <div class="col-md-6">*@
                <pre class="brush: js">
        function fizzBuzz(numberOne, numberTwo) {
            var fizzBuzzArray = [];
            if (1 <= numberOne <= 100 && 1 <= numberTwo <= 100) {
                for (var i = 1, l = 0; i <= 100; i++, l++) {
                    if ((i % numberOne) == 0 && (i % numberTwo) == 0) {
                        fizzBuzzArray[l] = ' ' + 'FizzBuzz' + ' ';
                    }
                    else if ((i % numberOne) == 0) {
                        fizzBuzzArray[l] = ' ' + 'Fizz' + ' ';
                    }
                    else if ((i % numberTwo) == 0) {
                        fizzBuzzArray[l] = ' ' + 'Buzz' + ' ';
                    }
                    else {
                        fizzBuzzArray[l] = ' ' + i + ' ';
                    }
                }
                return fizzBuzzArray;
            }
            else {
                return 'Invalid input';
            }  
        }
                                        </pre>
            </div>
            <div class="modal-footer no-margin">
            </div>
        </div>
    </div>
</div>
<!-- /Modal3 -->

<!-- Modal4 -->
<div class="modal fade" id="palindrome" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h1 class="modal-title" id="myModalLabel">Palindrome</h1>
            </div>
            <div class="modal-body col-sm-12">
                @*<h3>Palindrome</h3>*@
                <h5>Please enter a word to see if it is a palindrome or not</h5>
                <input type="text" id="inputFour" />
                <button onclick="problemFour()">Submit</button>
                <p id="resultFour">Result will show up here</p>
                @*<div class="row">
                    <div class="col-md-6">*@
                <pre class="brush: js">
        function palindromeFinder(word) {
            var wordReverse = '';
            
            if (word == '') {
                return 'Please write something before submitting.'
            } else {
                
                var lowercaseLetters = 'abcdefghijklmnopqrstuvwxyz';
                var capitalLetters = 'ABCDEFGHIJKLMNOPQRSTUVWQYZ';
                for (var i = 0; i < capitalLetters.length; i++) {   
                    for (var j = 0; j < word.length; j++) {
                        if (inputFour[j] === capitalLetters[i]) {
                            word = word.substr(0, j) + lowercaseLetters[i] + word.substr(j + 1);
                        }
                    }
                }
                for (var i = word.length - 1; i >= 0; i--) {
                    wordReverse += word[i];
                }
                
                if (word === wordReverse) {
                    return 'This word is a palindrome, congrats!';
                }
                
                else {
                    return 'This word is not a palindrome, sorry!';
                }
            }
        }
                                        </pre>
            </div>
            <div class="modal-footer no-margin">
            </div>
        </div>
    </div>
</div>
