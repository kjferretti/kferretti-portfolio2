                        <p id="resultThree">Result will show up here</p>
                        @*<div class="row">
                <div class="col-md-6">*@
                        <pre class="brush: js">
function fizzBuzz(numberOne, numberTwo) {
    var fizzBuzzArray = [];
    if (1 <= numberOne <= 100 && 1 <= numberTwo <= 100) {
        for (var i = 1, l = 0; i <= 100; i++, l++) {
            if ((i % numberOne) == 0 && (i % numberTwo) == 0) {
                fizzBuzzArray[l] = ' ' + 'FizzBuzz' + ' ';
            }
            else if ((i % numberOne) == 0) {
                fizzBuzzArray[l] = ' ' + 'Fizz' + ' ';
            }
            else if ((i % numberTwo) == 0) {
                fizzBuzzArray[l] = ' ' + 'Buzz' + ' ';
            }
            else {
                fizzBuzzArray[l] = ' ' + i + ' ';
            }
        }
        return fizzBuzzArray;
    }
    else {
        return 'Invalid input';
    }  
}
                            </pre>
