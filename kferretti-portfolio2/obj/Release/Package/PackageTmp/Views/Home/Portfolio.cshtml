                        <p id="resultTwo">Result will show up here</p>
                        @*<div class="row">
                <div class="col-md-6">*@
                        <pre class="brush: js">
function factorialFinder(input) {
    if (input < 0 || input > 170) {
        return 'Invalid input';
    }
    else if (input > 0) {
        var factorial = 1;
        while (input != 0) {
            factorial *= input;
            input = input - 1;
        }
        return 'Factorial: ' + factorial;
    }
    else {
        return 'Factorial: ' + 1;
    }
}
                            </pre>
